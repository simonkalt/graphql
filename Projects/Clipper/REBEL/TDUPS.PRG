SET ECHO OFF
SET TALK OFF
CLEAR
? "Twin Cities Dup Item Number Counter"
? "David Grant, Inc.  -  1995"
USE RDINV
INDEX ON DESCEND(TITEMNO) TO TTEMP 
SET INDEX TO TTEMP
CNT = 0
GO TOP
MTIT = "~@#$%^"
X := {}
DO WHILE !EOF() .AND. !EMPTY(TITEMNO)
  IF TITEMNO = MTIT
    AADD( X,TITEMNO )
    CNT = CNT + 1
  ENDIF
  MTIT = TITEMNO
  SKIP
ENDDO
IF LEN(X) > 0 
  ? "DUPS FOUND... PRINTING..."
  SET DEVICE TO PRINT
  SET PRINT ON
  FOR I := 1 TO LEN(X)
    ? X[I]
  NEXT I
  ? "*END OF LIST*"
  EJECT
  SET PRINT OFF
  SET DEVICE TO SCREEN
ELSE
  ? "NO DUPS FOUND."
ENDIF
? "DONE.  "
  
